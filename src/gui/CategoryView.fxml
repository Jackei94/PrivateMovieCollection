<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CategoryViewController">
   <children>
      <TextField fx:id="categoryName" layoutX="140.0" layoutY="87.0" />
      <Button fx:id="categoryCancel" layoutX="364.0" layoutY="148.0" mnemonicParsing="false" onAction="#categoryCancelButton" text="Cancel" />
      <Button fx:id="categorySave" layoutX="289.0" layoutY="148.0" mnemonicParsing="false" onAction="#categorySaveButton" text="Save" />
      <Label fx:id="newOrEditCategory" layoutX="140.0" layoutY="15.0" text="Category...">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="91.0" text="Name" />
   </children>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</AnchorPane>
